<Window x:Class="PL.WorkerMenu.Product.Print"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Print" Height="400" Width="600">
    <Grid>
        <DockPanel>
            <StackPanel DockPanel.Dock="Top" Name="panel"/>
            <Button DockPanel.Dock="Bottom" Name="refresh" Content="Refresh" Width="100" Height="25" Click="refresh_Click"/>
        </DockPanel>
    </Grid>
</Window>
